{
    "builds": [
        {
            "src": "src/medical/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/medical/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "medical.settings",
        "ALLOWED_HOSTS": ".vercel.app"
    }
} 